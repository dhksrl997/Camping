<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="root.*" template="/WEB-INF/view/inc/layout.jsp">
		<put-attribute name="css" value="" />
		<put-attribute name="header" value="/WEB-INF/view/inc/header.jsp" />
		<put-attribute name="main" value="/WEB-INF/view/{1}.jsp" />
	</definition>
		<definition name="camp.*" template="/WEB-INF/view/inc/layout.jsp">
		<put-attribute name="css" value="/css/camp/list.css" />
		<put-attribute name="header" value="/WEB-INF/view/inc/header.jsp" />
		<put-attribute name="main" value="/WEB-INF/view/camp/{1}.jsp" />
	</definition>
</tiles-definitions>